<?xml version="1.0" encoding="utf-8" ?>
<!-- XSD manual extracted from package NLog.Schema: https://www.nuget.org/packages/NLog.Schema-->
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xsi:schemaLocation="NLog NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true">

    <targets>
        <target xsi:type="File" name="trace_logs_file" fileName="${basedir}/Logs/${shortdate}-trace.log" />
        <target xsi:type="File" name="debug_logs_file" fileName="\logs\debug.log" />
    </targets>

    <rules>
        <logger name="*" minlevel="Trace" writeTo="trace_logs_file" />
        <logger name="*" minlevel="Debug" writeTo="debug_logs_file" />
    </rules>
</nlog>